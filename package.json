{
  "name": "fb-scraper",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "rm -rf dist && tsc",
    "postbuild": "npm run copy-views && npm run copy-json",
    "copy-views": "cp -r ./src/views/ ./dist",
    "copy-json": "cp ./package.json ./dist",
    "start": "node dist/index.js",
    "lint": "npx eslint . --ext .ts",
    "seed": "npm run update-teams && npm run update-teams-stats && npx ts-node src/scripts/seedPlayers.ts && npm run update-players-stats",
    "update-all": "npm run update-teams && npm run update-teams-stats && npm run update-players-stats",
    "update-teams": "npx ts-node src/scripts/updateTeams.ts",
    "update-teams-stats": "npx ts-node src/scripts/updateTeamStats.ts",
    "update-players-stats": "npx ts-node src/scripts/updatePlayerStats.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.14",
    "@types/hbs": "^4.0.1",
    "@types/jsdom": "^20.0.1",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.11",
    "@types/nodemailer": "^6.4.7",
    "@types/nodemailer-express-handlebars": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.1",
    "prisma": "^4.7.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "axios": "^1.2.1",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "hbs": "^4.2.0",
    "jsdom": "^20.0.3",
    "nodemailer": "^6.8.0",
    "nodemailer-express-handlebars": "^6.0.0"
  }
}
